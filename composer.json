{
  "name": "mypleasureteam/CASE",
  "description": "The PHP based website for MyPleasu.re users.",
  "private": true,
  "license": "All Rights Reserved MyPleasu.re 2015",
  "type": "project",
  "require": {
    "graham-campbell/throttle": "~2.0",
    "doctrine/dbal": "2.3.4",
    "jenssegers/mongodb": "~2.0",
    "ext-memcached": "*"
  },
  "require-dev": {
    "heroku/heroku-buildpack-php": "*",
    "phpunit/phpunit": "3.7.*",
    "way/phpunit-wrappers": "dev-master",
    "way/generators": "2.*",
    "way/laravel-test-helpers": "dev-master",
    "fzaninotto/faker": "1.5.*@dev"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php",
      "app/services"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
